pipeline{
    agent any

    parameters {
        string(defaultValue: "", description: 'Passed from job build Pet clinic', name: 'DOCKERTAG')
    }

    stages{
        stage("Clone Repository"){
                checkout scm
        }
        stage("Parametr Print"){
            steps{
                sh 'echo Here is the passed parametr ${DOCKERTAG}'
            }
        }
    }
}